<div class="row mt-2">
   <div class="col">
      <nav aria-label="breadcrumb">
         <ol class="breadcrumb">
           <li class="breadcrumb-item"><a href="#">Administração</a></li>
           <li class="breadcrumb-item active" aria-current="page">Grupo</li>
         </ol>
       </nav>
   </div>
</div>

<div class="row">
   <div class="col">
      <ul class="nav nav-tabs" id="tab" role="tablist">
         <li class="nav-item">
            <a class="nav-link active" id="principal-tab" role="tab" data-toggle="tab" data-selected="principal-tab" href="#principal-tab-content"
               aria-controls="principal" aria-selected="true">Principal</a>
         </li>
         <li class="nav-item">
            <a class="nav-link" id="modelo-tab" role="tab" data-toggle="tab" data-selected="modelo-tab" href="#modelo-tab-content"
               aria-controls="modelo" aria-selected="false">Modelo</a>
         </li>
      </ul>
      <div class="tab-content">
         <div class="tab-pane fade show active" id="principal-tab-content">
            <%- include('_formulario'); %>
         </div>
         <div class="tab-pane fade" id="modelo-tab-content">
            <div class="row">
               <div class="col my-2">
                  <div class="row">
                     <div class="col my-2">
                        <textarea id="html" rows="10" cols="80">
                        </textarea>
                     </div>
                  </div>
               </div>
            </div>
         </div>
      </div>
   </div>
</div>

<div class="botbar">
   <button type="button" class="btn btn-primary btn-sm">Confirmar</button>
   <button type="button" class="btn btn-outline-secondary btn-sm">Cancelar</button>
</div>
