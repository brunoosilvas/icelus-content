<div class="row mt-2">
   <div class="col">
      <nav aria-label="breadcrumb">
         <ol class="breadcrumb">
           <li class="breadcrumb-item"><a href="#">Administração</a></li>
           <li class="breadcrumb-item active" aria-current="page">Upload</li>
         </ol>
       </nav>
   </div>
</div>

<div class="row">
   <div class="col-6 col-md-4 col-lg-3">
      <ul class="list-group">
         <% for (diretorio of data.diretorios) { %>
            <% if (diretorio.base) { %>
               <a href="<%=diretorio.base%>" class="list-group-item list-group-item-action">
                  <div class="d-flex align-items-center">
                     <div class="p-1 mr-2 flex-shrink-1 bd-highlight"><i class="fa fa-exclamation-triangle"></i></div>
                     <div class="w-100 bd-highlight">ops não encontrado</div>
                  </div>
               </a>
            <% } else { %>
               <% if (diretorio.anterior) { %>
                  <a href="<%=diretorio.link%>" class="list-group-item list-group-item-action">
                     <div class="d-flex align-items-center">
                        <div class="p-1 mr-2 flex-shrink-1 bd-highlight"><i class="fa fa-folder-open"></i></div>
                        <div class="w-100 bd-highlight">...</div>
                     </div>
                  </a>
               <% } else { %>
                  <a href="<%=diretorio.link%>" class="list-group-item list-group-item-action">
                     <div class="d-flex align-items-center">
                        <div class="p-1 mr-2 flex-shrink-1 bd-highlight"><i class="fa fa-folder"></i></div>
                        <div class="w-100 bd-highlight"><%=diretorio.nome%></div>
                     </div>
                  </a>
               <% } %>
            <% } %>
         <% } %>
       </ul>
   </div>
   <div class="col-6 col-md-8 col-lg-9">
      <div class="custom-file">
         <input type="file" multiple="multiple" class="custom-file-input" id="file">
         <label class="custom-file-label" for="file" data-browse="Procurar">Selecione...</label>
      </div>
   </div>
</div>
